<div class="col-xs-10 col-xs-offset-1 panel">
  <h2><%= @place.name %></h2>
  <i><%= @place.address %></i>
  <br>
  <p><%= @place.description %></p>
  <div>
    <%= link_to '< back', root_path %>
    <% if current_user && current_user == @place.user %>
      <%= link_to 'edit', edit_place_path(@place), class: 'pull-right' %>
    <% end %>
  </div>
</div>
